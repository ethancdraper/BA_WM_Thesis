sub_list="/Users/ethandraper/Desktop/BA_Thesis/UCLA/Subjects/BIPOLAR/sub_list"

while read -r ID; do
    path="/Users/ethandraper/Desktop/BA_Thesis/UCLA/Subjects/BIPOLAR/sub${ID}"
    cd "sub-${ID}"
    invwarp -w /func/sub-${ID}_task-rest_bold.feat/reg/example_func2standard_warp.nii.gz -o standard2example_func_warp -r /func/sub-${ID}_task-rest_bold.feat/example_func.nii.gz
    echo "Standard2example_func has been created for $ID"
    cd ..
done < "$sub_list"
echo "Standard2example_func has been created for all participants in sub_list"